@model IEnumerable<ClassLibrary.Models.Content>
@if (Model != null && Model.Count() > 0)
{
    <h2><a href="#">Video</a></h2>
    <div class="wrapper-video-slider">
        <div class="video-slider owl-carousel owl-theme">
            @foreach (var item in Model)
            {
                <div class="item">
                    <a href="@item.alias" class="thumbnail">
                        <img src="@item.thumbnail" alt="" />
                    </a>
                </div>
            }
        </div>
    </div>
}

